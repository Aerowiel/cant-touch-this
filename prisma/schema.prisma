// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Score {
  id         String   @id @default(uuid())
  seed       String
  pseudonyme String
  score      Float
  replay     String?
  createdAt  DateTime @default(now())
}
