{
  "name": "cant-touch-this",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "deploy": "fly deploy --remote-only",
    "dev": "npm-run-all --parallel \"dev:*\"",
    "dev:start": "remix dev",
    "zdev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:css": "npm run build:css && node-sass -rw ./app/styles -o ./app/styles",
    "build": "npm run build:css && remix build",
    "build:css": "node-sass ./app/styles -o ./app/styles --output-style compressed",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@remix-run/css-bundle": "^2.0.0",
    "@remix-run/express": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "lzutf8": "^0.6.3",
    "morgan": "^1.10.0",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
